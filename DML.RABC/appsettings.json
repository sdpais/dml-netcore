{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {
    "Using": [ "Serilog.Sinks.PostgreSQL.Configuration" ],
    "MinimumLevel": "Information",
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "User ID=pgappadmin;Password=5zHFLfbHaewa;Host=hp-dev1;Port=55433;Database=serilog_logs",
          "tableName": "rbac_logs",
          "configurationPath": "ColumnsSectionHolder:AnotherSubSection",
          "needAutoCreateTable": true
        }
      }
    ]
  },
  "ColumnsSectionHolder": {
    "AnotherSubSection": {
      "Columns": {
        "message": "RenderedMessageColumnWriter",
        "level": {
          "Name": "LevelColumnWriter",
          "Args": {
            "renderAsText": true,
            "dbType": "Varchar"
          }
        },
        "raise_date": "TimestampColumnWriter",
        "exception": "ExceptionColumnWriter"
      }
    }
  },
  "ConnectionStrings": {
    "RBACdb": "Server=hp-dev1:5433;Database=rbac;User Id=pgappadmin;Password=5zHFLfbHaewa;"
  }
}